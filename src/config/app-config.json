{
  "app_settings": {
    "detection_method": "hybrid",
    "debug_mode": false,
    "camera": {
      "preferred_resolution": [1280, 720],
      "facing_mode": "environment",
      "constraints": {
        "width": { "ideal": 1280 },
        "height": { "ideal": 720 },
        "frameRate": { "ideal": 30 }
      }
    },
    "ar": {
      "world_tracking": true,
      "lighting_estimation": true,
      "plane_detection": true
    },
    "performance": {
      "max_fps": 30,
      "adaptive_quality": true,
      "battery_optimization": true
    }
  },
  "detection_settings": {
    "template_matching": {
      "algorithm": "ORB",
      "max_features": 500,
      "match_ratio": 0.75,
      "min_match_count": 10
    },
    "ml_inference": {
      "model_format": "tfjs",
      "confidence_threshold": 0.8,
      "batch_size": 1,
      "warm_up": true
    },
    "ocr_detection": {
      "language": "eng",
      "confidence_threshold": 0.3,
      "processing_timeout": 10000,
      "char_whitelist": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789 "
    },
    "hybrid_detection": {
      "template_weight": 0.7,
      "ocr_weight": 0.3,
      "agreement_bonus": 0.15,
      "high_confidence_bonus": 0.1
    }
  },
  "ui_settings": {
    "show_detection_overlay": true,
    "show_fps_counter": false,
    "haptic_feedback": true,
    "voice_feedback": false
  },
  "supported_products": [
    "zyn-can-1",
    "zyn-can-2"
  ]
}